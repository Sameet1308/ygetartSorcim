from mstrio.connection import Connection
from mstrio.project_objects import Cube

# Connect to your MicroStrategy environment
base_url = 'https://your-microstrategy-server.com/MicroStrategyLibrary/api'
username = 'your_username'
password = 'your_password'
project_name = 'your_project_name'

conn = Connection(base_url=base_url, username=username, password=password, project_name=project_name, login_mode=1)

# List cubes in the project
cubes = Cube.list(connection=conn)

# Print details about each cube
for cube in cubes:
    print(f"Cube ID: {cube.id}, Cube Name: {cube.name}")

# Disconnect (optional, good practice for session management)
conn.close()